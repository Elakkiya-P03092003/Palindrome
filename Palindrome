#include <stdio.h>

int main()
{
    int l=0,flag=0;
    char str[20];
    scanf("%[^\n]s",str);
    for(int i=0;str[i]!='\0';i++)
    l++;
    for(int i=0,j=l-1;i<j;i++,j--){
            if(str[i]!=str[j])
            flag=1;
            break;}
if(flag==0)
    printf("Palindrome");

else
    printf("Not a palindrome");
return 0;}
